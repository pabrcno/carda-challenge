{
  "name": "carda-challenge",
  "version": "1.0.0",
  "description": "Node.js API scaffold with TypeScript, ts-rest, Drizzle ORM, and Zod",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx src/scripts/seed.ts",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:db": "docker-compose up -d postgres",
    "docker:pgadmin": "docker-compose up -d postgres pgadmin",
    "docker:build": "docker-compose build",
    "docker:prod": "docker-compose --profile production up -d",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:performance": "jest --testPathPattern=performance",
    "test:all": "tsx src/__tests__/test-runner.ts",
    "test:quick": "tsx src/__tests__/test-runner.ts --quick"
  },
  "keywords": [
    "typescript",
    "ts-rest",
    "drizzle",
    "zod",
    "api",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/express": "^3.52.1",
    "@types/bull": "^3.15.9",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "express": "^4.21.2",
    "ioredis": "^5.4.1",
    "pg": "^8.16.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.31.4",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
